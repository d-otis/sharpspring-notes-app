<%= form_with model: @note do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %>

  <br>

  <%= form.label :body %>
  <%= form.text_area :body %>
  <br>
  <%= form.label :pinned, "Pin Note" %>
  <%= form.check_box :pinned %>
  <br>
  <%= form.hidden_field :user_id, :value => current_user.id %>
  <br>
  <%= form.submit %>
<% end %>